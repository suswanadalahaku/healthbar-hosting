{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/img/(.*)",
            "dest": "/public/img/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://example-atomik.vercel.app/",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stack",
        "LOG_LEVEL": "debug",
        "SESSION_DRIVER": "cookie",
        "SESSION_LIFETIME": "120",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "127.0.0.1",
        "DB_PORT": "3306",
        "DB_DATABASE": "healthbar",
        "DB_USERNAME": "root",
        "DB_PASSWORD": "",
        "MAIL_MAILER": "log",
        "MAIL_HOST": "127.0.0.1",
        "MAIL_PORT": "2525",
        "MAIL_USERNAME": "null",
        "MAIL_PASSWORD": "null",
        "MAIL_FROM_ADDRESS": "hello@example.com",
        "MAIL_FROM_NAME": "Laravel",
        "APP_KEY": "base64:4dKk3cXp6l1IVBzeGlj9VcYmUCN0k+oJOLhYBmyAhtc=",
        "APP_TIMEZONE": "Asia/Jakarta"
    },
    "outputDirectory": "public"
}
